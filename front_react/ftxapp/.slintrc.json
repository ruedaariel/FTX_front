{
  "root": true,
  "plugins": ["import"],
  "rules": {
    
    "import/no-unresolved": ["error", { "caseSensitive": true }], 
    
    
    "import/no-duplicates": "error", 
    "import/named": "error", 
    
    
    "no-unused-vars": "off", 
    "no-undef": "off", 
    
    "react/jsx-uses-vars": "off" 
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".css", ".ts", ".tsx"] 
      }
    }
  }
}




